# .env file

# Database initialization
INFLUXDB_DB=k6       # Name of the default database (bucket in 2.x)
INFLUXDB_USER=k6            # Admin username
INFLUXDB_PASSWORD=k6      # Admin password

# Optional: If you need a standard user besides the admin
INFLUXDB_ADMIN_USER=admin                      # Standard user username
INFLUXDB_ADMIN_PASSWORD=nine@123     # Standard user password

# Grafana secrets
GRAFANA_ADMIN_USER=dogpackapp
GRAFANA_ADMIN_PASSWORD=dogpackapp12&